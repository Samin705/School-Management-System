@model IEnumerable<Mid_SchoolManagementSystem.Models.DataAccess.superadmin>

@{
    ViewBag.Title = "ListSuperAdmin";
    Layout = "~/Views/Shared/_SuperLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListSuperAdmin</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "CreateSuperAdmin")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.superadminid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.superadminname)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.superadminid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.superadminname)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditSuperAdmin", new { id = item.id }) |
                    @Html.ActionLink("Delete", "DeleteSuperAdmin", new { id = item.id })
                </td>
            </tr>
        }

    </table>
</body>
</html>